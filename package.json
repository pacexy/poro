{
  "name": "poro",
  "version": "0.3.2",
  "description": "Integrate official Riot's League of Legends API with Leaguepedia API.",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write src/**/*.{js}",
    "generate": "node src/generator/generator.js"
  },
  "husky": {
