{
	"name": "poro",
	"version": "0.5.3",
	"description": "Integrate official Riot's League of Legends API with Leaguepedia API.",
	"main": "build/src/index.js",
	"scripts": {
		"format": "prettier --write src/**/*.ts",
		"generate": "ts-node src/generator/generator.ts",
		"build": "tsc",
		"prepare": "npm run build"
	},
	"files": [
    "build/src",
    "!build/src/**/*.map"
  ],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.ts": [
			"prettier --write",
			"eslint --fix"
		]
	},
	"keywords": [
		"api",
		"league of legends",
		"lol",
		"riot",
		"leaguepedia"
	],
	"author": "pacexy",
	"license": "MIT",
	"homepage": "https://github.com/pacexy/poro",
	"repository": {
		"type": "git",
		"url": "https://github.com/pacexy/poro"
	},
	"bugs": {
		"url": "https://github.com/pacexy/poro/issues"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"cheerio": "^1.0.0-rc.3",
		"lodash": "^4.17.21"
	},
	"devDependencies": {
		"@types/cheerio": "^0.22.22",
		"@types/lodash": "^4.14.171",
		"@types/node": "^16.4.0",
		"@types/nunjucks": "^3.1.3",
		"@types/prettier": "^2.3.2",
		"hitoka": "^0.3.0",
		"husky": "^4.2.5",
		"lint-staged": "^10.2.11",
		"nunjucks": "^3.2.2",
		"prettier": "^2.3.2",
		"ts-node": "^10.1.0",
		"typescript": "^4.3.5"
	}
}
